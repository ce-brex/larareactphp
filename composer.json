{
    "name": "tyea/larareactphp",
    "description": "A Laravel package for using ReactPHP to serve your application",
    "keywords": [
    	"laravel",
    	"react",
    	"reactphp",
    	"serve"
    ],
	"homepage": "https://github.com/tyea/larareactphp",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Yeadon",
			"homepage": "http://www.tomyeadon.com"
		}
	],
    "require": {
        "php": ">=5.6.4",
        "symfony/http-foundation": "~3.2",
        "symfony/http-kernel": "~3.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.4",
        "guzzlehttp/guzzle": "^6.2",
        "react/http": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelReactPHP\\": "src/"
        }
    }
}
